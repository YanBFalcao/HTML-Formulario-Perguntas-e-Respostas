<script type="text/javascript">
	 
    function validacao() // Função para validação de Radio Button
    {

// 1) Qual o seu grau de satisfação com os cursos abaixo?

//
        var validaA = false;
		var validaB = false;
		var validaC = false;
		var validaD = false;
		var validaE = false;

        var SatisfacaoS = document.formularioSatisfacao.satisfacaoViolao; // fazendo validação linha do radio button
        var SatisfacaoT = document.formularioSatisfacao.satisfacaoCanto; // fazendo validação linha do radio button
		var SatisfacaoU = document.formularioSatisfacao.satisfacaoGuitarra; // fazendo validação linha do radio button
		var SatisfacaoV = document.formularioSatisfacao.satisfacaoBaixo; // fazendo validação linha do radio button
		var SatisfacaoX = document.formularioSatisfacao.satisfacaoBateria; // fazendo validação linha do radio button

		for(var a = 0; a < SatisfacaoS.length; a++)
        {
            if(SatisfacaoS[a].checked)
            {
            	validaA = true; // Valida A

            	if(validaA == true)
            	{
            		for(var b = 0; b < SatisfacaoT.length; b++)
                	{
                		if(SatisfacaoT[b].checked)
                		{
                			validaB = true; // Valida B

                			if(validaB == true)
            				{
            					for(var c = 0; c < SatisfacaoU.length; c++)
                				{
                					if(SatisfacaoU[c].checked)
                					{
                						validaC = true; // Valida C
                					
                						if(validaC == true)
            							{
            								for(var d = 0; d < SatisfacaoV.length; d++)
                							{
                								if(SatisfacaoV[d].checked)
                								{
                									validaD = true;  // Valida D
                			
													if(validaD == true)
            										{
            											for(var e = 0; e < SatisfacaoX.length; e++)
                										{
                											if(SatisfacaoX[e].checked)
                											{
                												validaE = true;  // Valida E
                												break;
                											}
                										}
               										}
                								}
                							}
               							}
                					}
                				}
               				}
                		}
                	}
                }
            }
        }

        if((validaA == true) && (validaB == true) && (validaC == true) && (validaD == true) && (validaE == true))
        {
            alert("Validação de Campos com sucesso!");
        }
        else
        {
            alert("Falta campo a validar!");
            //return false;
        } // Script de funcionamento de validação de botão 1
 
//

		// ========================================================================================================

		// 2) Qual a sua nota para os professores abaixo?

		var validaF = false;
		var validaG = false;
		var validaH = false;
		var validaI = false;
		var validaJ = false;

        var ProfessoresS = document.formularioProfessores.ProfessorPedro; // fazendo validação linha do radio button
        var ProfessoresT = document.formularioProfessores.ProfessorGabriel; // fazendo validação linha do radio button
		var ProfessoresU = document.formularioProfessores.ProfessorDaniel; // fazendo validação linha do radio button
		var ProfessoresV = document.formularioProfessores.ProfessoraMonica; // fazendo validação linha do radio button
		var ProfessoresX = document.formularioProfessores.ProfessoraLara; // fazendo validação linha do radio button

        for(var f = 0; f < ProfessoresS.length; f++)
        {
            if(ProfessoresS[f].checked)
            {
            	validaF = true; // Valida A

            	if(validaF == true)
            	{
            		for(var g = 0; g < ProfessoresT.length; g++)
                	{
                		if(ProfessoresT[g].checked)
                		{
                			validaG = true; // Valida B

                			if(validaG == true)
            				{
            					for(var h = 0; h < ProfessoresU.length; h++)
                				{
                					if(ProfessoresU[h].checked)
                					{
                						validaH = true; // Valida C
                					
                						if(validaH == true)
            							{
            								for(var i = 0; i < ProfessoresV.length; i++)
                							{
                								if(ProfessoresV[i].checked)
                								{
                									validaI = true;  // Valida D
                			
													if(validaI == true)
            										{
            											for(var j = 0; j < ProfessoresX.length; j++)
                										{
                											if(ProfessoresX[j].checked)
                											{
                												validaJ = true;  // Valida E
                												break;
                											}
                										}
               										}
                								}
                							}
               							}
                					}
                				}
               				}
                		}
                	}
                }
            }
        }

        if((validaF == true) && (validaG == true) && (validaH == true) && (validaI == true) && (validaJ == true))
        {
            alert("Validação de Campos com sucesso!");
        }
        else
        {
            alert("Falta campo a validar!");
            //return false;
        }

        // ========================================================================================================  // Script de funcionamento de validação de botão 2

//
        // 3)	Qual outro curso tem vontade de aprender a realizar?

		var validaK = false;
		var validaL = false;
		var validaM = false;
		var validaN = false;
		var validaO = false;

        var CursoS = document.formularioCursosRealizar.CursoViolao; // fazendo validação linha do radio button
        var CursoT = document.formularioCursosRealizar.CursoCanto; // fazendo validação linha do radio button
		var CursoU = document.formularioCursosRealizar.CursoGuitarra; // fazendo validação linha do radio button
		var CursoV = document.formularioCursosRealizar.CursoBaixo; // fazendo validação linha do radio button
		var CursoX = document.formularioCursosRealizar.CursoBateria; // fazendo validação linha do radio button

        for(var k = 0; k < CursoS.length; k++)
        {
            if(CursoS[k].checked)
            {
            	validaK = true; // Valida A

            	if(validaK == true)
            	{
            		for(var l = 0; l < CursoT.length; l++)
                	{
                		if(CursoT[l].checked)
                		{
                			validaL = true; // Valida B

                			if(validaL == true)
            				{
            					for(var m = 0; m < CursoU.length; m++)
                				{
                					if(CursoU[m].checked)
                					{
                						validaM = true; // Valida C
                					
                						if(validaM == true)
            							{
            								for(var n = 0; n < CursoV.length; n++)
                							{
                								if(CursoV[n].checked)
                								{
                									validaN = true;  // Valida D
                			
													if(validaN == true)
            										{
            											for(var o = 0; o < CursoX.length; o++)
                										{
                											if(CursoX[o].checked)
                											{
                												validaO = true;  // Valida E
                												break;
                											}
                										}
               										}
                								}
                							}
               							}
                					}
                				}
               				}
                		}
                	}
                }
            }
        }

        if((validaK == true) && (validaL == true) && (validaM == true) && (validaN == true) && (validaO == true))
        {
            alert("Validação de Campos com sucesso!");
        }
        else
        {
            alert("Falta campo a validar!");
            //return false;
        } // Script de funcionamento de validação de botão 3

//
    
    // 4) Recomendaria os cursos abaixo?

		var validaP = false;
		var validaQ = false;
		var validaR = false;
		var validaS = false;
		var validaT = false;

        var RecomendacaoCursoS = document.formularioRecomendacao.recomendacaoCursoViolao; // fazendo validação linha do radio button
        var RecomendacaoCursoT = document.formularioRecomendacao.recomendacaoCursoCanto; // fazendo validação linha do radio button
		var RecomendacaoCursoU = document.formularioRecomendacao.recomendacaoCursoGuitarra; // fazendo validação linha do radio button
		var RecomendacaoCursoV = document.formularioRecomendacao.recomendacaoCursoBaixo; // fazendo validação linha do radio button
		var RecomendacaoCursoX = document.formularioRecomendacao.recomendacaoCursoBateria; // fazendo validação linha do radio button

        for(var p = 0; p < RecomendacaoCursoS.length; p++)
        {
            if(RecomendacaoCursoS[p].checked)
            {
            	validaP = true; // Valida P

            	if(validaP == true)
            	{
            		for(var q = 0; q < RecomendacaoCursoT.length; q++)
                	{
                		if(RecomendacaoCursoT[q].checked)
                		{
                			validaQ = true; // Valida Q

                			if(validaQ == true)
            				{
            					for(var r = 0; r < RecomendacaoCursoU.length; r++)
                				{
                					if(RecomendacaoCursoU[r].checked)
                					{
                						validaR = true; // Valida R
                					
                						if(validaR == true)
            							{
            								for(var s = 0; s < RecomendacaoCursoV.length; s++)
                							{
                								if(RecomendacaoCursoV[s].checked)
                								{
                									validaS = true;  // Valida S
                			
													if(validaS == true)
            										{
            											for(var t = 0; t < RecomendacaoCursoX.length; t++)
                										{
                											if(RecomendacaoCursoX[t].checked)
                											{
                												validaT = true;  // Valida T
                												break;
                											}
                										}
               										}
                								}
                							}
               							}
                					}
                				}
               				}
                		}
                	}
                }
            }
        }

        if((validaP == true) && (validaQ == true) && (validaR == true) && (validaS == true) && (validaT == true))
        {
            alert("Validação de Campos com sucesso!");
        }
        else
        {
            alert("Falta campo a validar!");
            //return false;
        } // Script de funcionamento de validação de botão 3

//
    
    // 5)	Como você classifica os instrumentos?

		var validaU = false;
		var validaV = false;
		var validaX = false;
		var validaY = false;
		var validaZ = false;

        var instrumentoS = document.formularioInstrumentos.instrumentoViolao; // fazendo validação linha do radio button
        var instrumentoT = document.formularioInstrumentos.instrumentoCanto; // fazendo validação linha do radio button
		var instrumentoU = document.formularioInstrumentos.instrumentoGuitarra; // fazendo validação linha do radio button
		var instrumentoV = document.formularioInstrumentos.instrumentoBaixo; // fazendo validação linha do radio button
		var instrumentoX = document.formularioInstrumentos.instrumentoBateria; // fazendo validação linha do radio button

        for(var u = 0; u < instrumentoS.length; u++)
        {
            if(instrumentoS[u].checked)
            {
            	validaU = true; // Valida P

            	if(validaU == true)
            	{
            		for(var v = 0; v < instrumentoT.length; v++)
                	{
                		if(instrumentoT[v].checked)
                		{
                			validaV = true; // Valida Q

                			if(validaV == true)
            				{
            					for(var x = 0; x < instrumentoU.length; x++)
                				{
                					if(instrumentoU[x].checked)
                					{
                						validaX = true; // Valida R
                					
                						if(validaX == true)
            							{
            								for(var y = 0; y < instrumentoV.length; y++)
                							{
                								if(instrumentoV[y].checked)
                								{
                									validaY = true;  // Valida S
                			
													if(validaY == true)
            										{
            											for(var z = 0; z < instrumentoX.length; z++)
                										{
                											if(instrumentoX[z].checked)
                											{
                												validaZ = true;  // Valida T
                												break;
                											}
                										}
               										}
                								}
                							}
               							}
                					}
                				}
               				}
                		}
                	}
                }
            }
        }

        if((validaU == true) && (validaV == true) && (validaX == true) && (validaY == true) && (validaZ == true))
        {
            alert("Validação de Campos com sucesso!");
        }
        else
        {
            alert("Falta campo a validar!");
            //return false;
        } // Script de funcionamento de validação de botão 3


    } // Final function


